Algoritmo ejercicio1multi
	
	Definir operando1, operando2, resultado Como Real;
	
	Escribir Sin Saltar"Por favor,introduzca su primer operando";
	Leer operando1;
	
	Escribir Sin Saltar"Por favor introduzca su segundo operando";
	Leer operando2;
	
	resultado <- operando1 * operando2;
	
	Escribir "El resultado de la multiplicación es ", resultado;
FinAlgoritmo
