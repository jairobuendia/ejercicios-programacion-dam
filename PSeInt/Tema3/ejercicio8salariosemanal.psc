Algoritmo ejercicio8salariosemanal
	Definir horas, sueldohora, resultado Como Real;
	
	Escribir Sin Saltar"Por favor, introduce las horas trabajadas esta semana: ";
	Leer horas;
	
	sueldohora <- 12;
	
	resultado <- horas * sueldohora;
	
	Escribir "Su salario semanal es de: ",resultado, "euros";
FinAlgoritmo
