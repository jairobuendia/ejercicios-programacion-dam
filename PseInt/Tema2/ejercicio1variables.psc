Algoritmo ejercicio1variables
	Definir x, z Como Entero;
	Definir suma, resta, multiplicacion Como Entero;
	Definir division Como Real;
	x <- 144;
	z <- 999;
	suma <- x + z;
	resta <- x - z;
	division <- x / z;
	multiplicacion <- x * z;
	Escribir "suma: ", suma;
	Escribir "resta: ", resta;
	Escribir "division: ", division;
	Escribir "multiplicacion: ", multiplicacion;
FinAlgoritmo
