Algoritmo Pruebavariables
	// Ejemplo de variable real//
	Definir precio Como Real;
	precio <- 19.95;
	Escribir precio;
	precio <- 9.95;
	Escribir precio;
	
	//Ejemplo de variable entera//
	Definir alumnos Como Entero;
	alumnos<- 12;
	Escribir "Alumnos matriculados hasta hoy ",alumnos;
	alumnos<- 20;
	Escribir "Alumnos que se esperan ",alumnos;
FinAlgoritmo
