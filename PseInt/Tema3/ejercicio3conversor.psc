Algoritmo ejercicio3conversor
	
	Definir pesetas, conversor, resultado Como Real;
	
	Escribir Sin Saltar"Por favor,introduzca la cantidad en pesetas que desea convertir";
	Leer pesetas;
	
	conversor <- 166.386;
	
	resultado <- pesetas / conversor;
	
	Escribir "Su cantidad en euros es la siguiente ", resultado;
FinAlgoritmo
