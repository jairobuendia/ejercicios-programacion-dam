Algoritmo ejercicio5rectangulo
	
	Definir base, altura, resultado Como Real;
	
	Escribir "Programa para calcular el area de un rectángulo";
	
	Escribir Sin Saltar"Por favor, introduzca la base";
	Leer base;
	
	Escribir Sin Saltar"Por favor,introduzca la altura";
	Leer altura;
	
	resultado <- base * altura;
	
	Escribir "El area del rectángulo es la siguiente ", resultado;
FinAlgoritmo
