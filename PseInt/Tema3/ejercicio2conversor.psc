Algoritmo ejercicio2conversor
	
	Definir euros, conversor, resultado Como Real;
	
	Escribir Sin Saltar"Por favor,introduzca la cantidad en euros que desea convertir";
	Leer euros;
	
	conversor <- 166.386;
	
	resultado <- euros * conversor;
	
	Escribir euros, " euros son ", resultado " pesetas";
FinAlgoritmo
