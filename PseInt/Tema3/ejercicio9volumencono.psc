Algoritmo ejercicio9volumencono
	
	Definir radio, altura, resultado Como Real;
	
	
	Escribir Sin Saltar"Por favor, introduzca el radio";
	Leer radio;
	
	Escribir Sin Saltar"Por favor,introduzca la altura";
	Leer altura;
	
	resultado <- (1/3) * pi * radio * altura;
	
	Escribir "El volumen del cono es el siguiente ", resultado;
FinAlgoritmo
